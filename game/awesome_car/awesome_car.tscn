script/source = "extends AudioStreamPlayer3D


# Called every frame. 'delta' is the elapsed time since the previous frame.
func _process(_delta: float) -> void:
	var car = $\"..\"
	var speed = car.speed
	var wanted_pitch = speed / 80 + 0.5
	if car.burning_tire or not car.grounded:
		wanted_pitch += 0.5
	pitch_scale = move_toward(pitch_scale, wanted_pitch, _delta)
	pass
"

[sub_resource type="GDScript" id="GDScript_g6rhu"]
script/source = "extends AudioStreamPlayer3D



# Called every frame. 'delta' is the elapsed time since the previous frame.
func _process(_delta: float) -> void:
	var burning_tire = $\"..\".burning_tire
	if not playing and burning_tire:
		volume_db = -12.0
		play()
	var wanted_db = -10.0 if burning_tire else -60.0
	volume_db = move_toward(volume_db, wanted_db, 200.0 * _delta)
	if playing and volume_db < -50:
		stop()
"

[sub_resource type="GDScript" id="GDScript_vu0fh"]
script/source = "extends AudioStreamPlayer3D

func _process(delta: float) -> void:
	var desired_volume = -15.0 if $\"..\".offroad else -60.0
	volume_db = move_toward(volume_db, desired_volume, delta * 300.0)
	if volume_db < -50 and playing:
		stop()
	if volume_db > -50 and not playing:
		play()
		
"

